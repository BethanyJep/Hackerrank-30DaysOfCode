    "while n != 0:\n",
    "\tfactor = n // 2\n",
    "\tremainder = n - 2 * factor\n",
    "\tn = factor\n",
    "\tif remainder == 1:\n",
    "\t\tone_count += 1\n",
    "\t\tmax_one_count = max(max_one_count, one_count)\n",
    "\telse:\n",
    "\t\tone_count = 0\n",
